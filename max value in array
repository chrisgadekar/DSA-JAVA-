

public class main {
    public static void main(String[] args) {
       int[] arr = {10,20,45,7,18};
       System.out.println(largestVal(arr));
      
    }
    static int largestVal(int[] arr ){
        int max = arr[0];
        for (int i = 0; i < arr.length ; i++) {
            if (arr[i] > max ) {
                max = arr[i];
            }
            

        }
        return max;

    }
}
